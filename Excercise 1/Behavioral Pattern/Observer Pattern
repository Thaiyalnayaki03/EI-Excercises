import java.util.ArrayList;
import java.util.List;

interface Observer { void update(float temp); }

class WeatherData {
    private List<Observer> observers = new ArrayList<>();
    public void addObserver(Observer o) { observers.add(o); }
    public void setMeasurements(float temp) { for (Observer o : observers) o.update(temp); }
}

class Display implements Observer {
    public void update(float temp) { System.out.println("Temperature: " + temp); }
}

public class WeatherStation {
    public static void main(String[] args) {
        WeatherData weatherData = new WeatherData();
        Display display = new Display();
        weatherData.addObserver(display);
        weatherData.setMeasurements(25.5f);
    }
}
